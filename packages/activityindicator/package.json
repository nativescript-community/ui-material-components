{
    "name": "@nativescript-community/ui-material-activityindicator",
    "version": "4.0.1",
    "description": "Material ActivityIndicator component",
    "main": "./index",
    "sideEffects": false,
    "typings": "./index.d.ts",
    "scripts": {
        "tsc": "../../node_modules/.bin/cpy ../../src/activityindicator/index.d.ts ./ && ../../node_modules/.bin/tsc --skipLibCheck -d",
        "tsc-win": "../../node_modules/.bin/cpy ..\\..\\src\\activityindicator\\index.d.ts .\\ && ..\\..\\node_modules\\.bin\\tsc --skipLibCheck -d",
        "build": "npm run tsc && npm run build.angular",
        "build.angular": "cd angular && npm run build",
        "clean": "../../node_modules/.bin/rimraf ./*.d.ts ./*.js ./*.js.map"
    },
    "nativescript": {
        "platforms": {
            "android": "6.2.0",
            "ios": "6.2.0"
        }
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS"
    ],
    "author": {
        "name": "Martin Guillon",
        "email": "martin@akylas.fr"
    },
    "bugs": {
        "url": "https://github.com/Akylas/nativescript-material-components/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Akylas/nativescript-material-components",
    "readmeFilename": "README.md",
    "dependencies": {
        "@nativescript-community/ui-material-core": "^4.0.1"
    },
    "gitHead": "f0c903eb440eda20c2a9cd1243cb88d4a6313bcd"
}
