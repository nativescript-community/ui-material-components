{
	"name": "@nativescript-community/ui-material-appbar-angular",
	"main": "index.js",
	"ngPackage": {
		"dest":"../../../tempAngular/appbar",
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"@nativescript-community/ui-material-appbar": "ns-material-appbar"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "pnpm i && ../../../node_modules/.bin/rimraf ../../../tempAngular/appbar && ../../../node_modules/.bin/ng-packagr -p package.json && cd ../../../tempAngular/appbar && ../../node_modules/.bin/cpy '**/*' '../../packages/appbar/angular' --parents"
	},
	"devDependencies": {
		"tslib":"2.0.0",
		"@nativescript-community/ui-material-core": "file:../../../packages/core"
	}
}
