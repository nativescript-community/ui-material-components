{
	"name": "@nativescript-community/ui-material-button-angular",
	"main": "index.js",
	"ngPackage": {
		"dest": "../../../tempAngular/button",
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"@nativescript-community/ui-material-button": "ns-material-button"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "pnpm i && ../../../node_modules/.bin/rimraf ../../../tempAngular/button && ../../../node_modules/.bin/ng-packagr -p package.json && cd ../../../tempAngular/button && ../../node_modules/.bin/cpy '**/*' '../../packages/button/angular' --parents"
	},
	"devDependencies": {
		"tslib": "2.0.0",
		"@nativescript-community/ui-material-core": "file:../../../packages/core",
		"@nativescript-community/ui-material-button": "file:../../../packages/button"
	}
}
