{
	"name": "@nativescript-community/ui-material-ripple-angular",
	"main": "index.js",
	"ngPackage": {
		"dest": "../../../tempAngular/ripple",
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"@nativescript-community/ui-material-ripple": "ns-material-ripple"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "pnpm i && ../../../node_modules/.bin/rimraf ../../../tempAngular/ripple && ../../../node_modules/.bin/ng-packagr -p package.json && cd ../../../tempAngular/ripple && ../../node_modules/.bin/cpy '**/*' '../../packages/ripple/angular' --parents"
	},
	"devDependencies": {
		"tslib": "2.0.0",
		"@nativescript-community/ui-material-core": "file:../../../packages/core",
		"@nativescript-community/ui-material-ripple": "file:../../../packages/ripple"
	}
}
