{
	"name": "@nativescript-community/ui-material-slider-angular",
	"main": "index.js",
	"ngPackage": {
		"dest":"../../../tempAngular/slider",
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"@nativescript-community/ui-material-slider": "ns-material-slider"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "pnpm i && ../../../node_modules/.bin/rimraf ../../../tempAngular/slider && ../../../node_modules/.bin/ng-packagr -p package.json && cd ../../../tempAngular/slider && ../../node_modules/.bin/cpy '**/*' '../../packages/slider/angular' --parents"
	},
	"devDependencies": {
		"tslib":"2.0.0",
		"@nativescript-community/ui-material-core": "file:../../../packages/core",
		"@nativescript-community/ui-material-slider": "file:../../../packages/slider"
	}
}
