{
	"name": "@nativescript-community/ui-material-textfield-angular",
	"main": "index.js",
	"ngPackage": {
		"dest": "../../../tempAngular/textfield",
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"@nativescript-community/ui-material-textfield": "ns-material-textfield"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "pnpm i && ../../../node_modules/.bin/rimraf ../../../tempAngular/textfield && ../../../node_modules/.bin/ng-packagr -p package.json && cd ../../../tempAngular/textfield && ../../node_modules/.bin/cpy '**/*' '../../packages/textfield/angular' --parents"
	},
	"devDependencies": {
		"tslib": "2.0.0",
		"@nativescript-community/ui-material-core": "file:../../../packages/core",
		"@nativescript-community/ui-material-textfield": "file:../../../packages/textfield"
	}
}
